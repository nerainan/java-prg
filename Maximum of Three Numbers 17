import java.io.*;
import java.util.*;

public class Solution {
    public static int findLargest(int a, int b, int c){
        int max = a;
       
        if(b > max){
            max = b;
        }
       
        if(c > max){
            max = c;
        }
       
        return max;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
       
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
       
        int result = findLargest(a, b, c);
       
        System.out.print(result);
       
    }
}
