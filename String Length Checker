import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        
        String s = sc.nextLine();
        if(s == null){
            System.out.println(" Null not found");
        }
        else{
            System.out.println(s.length());
        }
        sc.close();
    }
}
