import java.io.*;
import java.util.*;

class Vehicle{
    String brand;
    
    void showBrand(){
        System.out.println("Brand:" + brand);
    }
    
}
class Car extends Vehicle{
    int speed ;
    
    void showSpeed(){
        System.out.println("Speed:" + speed);
        
    }
    
}
public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        Car C = new Car();
        
        
        C.brand = sc.nextLine();
        C.speed =sc.nextInt();
        
        C.showBrand();
        C.showSpeed();
    
    }
}
